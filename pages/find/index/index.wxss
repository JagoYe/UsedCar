/* pages/find/index/index.wxss */
page{
  width: 100%;
  height: 100%;
  background-color: #F2F2F2;
  font-size: 26rpx;
  font-family: 微软雅黑;
}
/* 整个页面 */
.all{
  width: 100%;
  height: 100%;
  background-color: #F2F2F2;
}
/*汽车服务*/
.service{
  width: 100%;
  height: auto;
  background-color: white;
  margin-top: 30rpx;
}
/*洗车服务*/
.carWash{
  width: 100%;
  height: 80rpx;
  border-bottom: solid gainsboro 1rpx;
}
/* 修车服务 */
.repair{
  width: 100%;
  height: 80rpx;
}
/*图片*/
.carWashimg{
  width: 15%;
  height: 80rpx;
  float: left;
  text-align: center;
}
.carWashtext{
  width: 70%;
  line-height: 80rpx;
  height: 80rpx;
  float: left;
}
.carWashicon{
  width: 10%;
  height: 80rpx;
  float: left;
  text-align: center;
}
.carWashicon image{
  width: 30%;
  height: 14rpx;
  margin-top: 33rpx;
  transform: rotate(-90deg);
}