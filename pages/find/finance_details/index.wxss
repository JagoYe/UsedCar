/* pages/find/carWash_details/index.wxss */
page{
  width: 100%;
  height: 100%;
  font-family: 微软雅黑;
  font-size: 26rpx;
}
/*大容器*/
.all{
  width: 100%;
  height: auto;
}
/*头部快*/
.header{
  width: 100%;
  height: 200rpx;
  position: relative;
}
.header image{
  width: 100%;
  height: 100%;
}
.title{
  width: 90%;
  height: 100%;
  line-height: 200rpx;
  position: absolute;
  top: 0;
  left: 5%;
  font-size: 30rpx;
  color: white;
}

/*二手车洗车列表页*/
.list{
  width: 100%;
  height: 150rpx;
  margin-top: 25rpx;
  border-bottom: 2rpx solid gray;} 
.listCar{
  width: 90%;
  height: 125rpx;}
.listCar .left{
  width: 30%;
  height: 125rpx;
  float: left;}
.listCar .left image{
  width: 100%;
  height: 100%;}          
.listCar .right{
  width: 68%;
  height: 125rpx;
  margin-left: 2%;
  float: left;}  
.listCar .right .details{
  width: 100%;
  height: 41rpx;
  line-height: 41rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;}
